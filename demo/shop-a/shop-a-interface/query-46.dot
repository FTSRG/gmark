digraph g {
	x0 [fillcolor="yellow", style="filled," shape=circle, label="?x0"];
	x1 [shape=circle, label="?x1"];
	x0 -> x1 [label="((birthDate-.gender))"];
	x2 [shape=circle, label="?x2"];
	x0 -> x2 [label="((expires-.like-.follows.gender)+(release-.like-.gender)+(priceValidUntil-.validForm.birthDate-.gender))"];
	x3 [shape=circle, label="?x3"];
	x0 -> x3 [label="((expires-.author.gender)+(purchaseDate-.purchaseFor.conductor.gender))"];
}
