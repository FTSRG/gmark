BEGIN
gmarkSubquery00(x, y) <- core:edge:edge(x1, publishedIn, x0), core:edge:edge(x1, publishedIn, x2), x = x0, y = x2.
gmarkSubquery00(x, y) <- core:edge:edge(x1, publishedIn, x0), core:edge:edge(x1, publishedIn, x2), x = x0, y = x2.
gmarkSubquery00(x, x) <- core:edge:edge(x, r, y).
gmarkSubquery00(y, y) <- core:edge:edge(x, r, y).
gmarkSubquery00(x, y) <- gmarkSubquery00(x, z), gmarkSubquery00(z, y).
gmarkSubquery01(x, y) <- core:edge:edge(x1, publishedIn, x0), core:edge:edge(x2, authors, x1), core:edge:edge(x2, authors, x3), core:edge:edge(x3, extendedTo, x4), x = x0, y = x4.
gmarkSubquery01(x, y) <- core:edge:edge(x1, publishedIn, x0), core:edge:edge(x1, extendedTo, x2), core:edge:edge(x3, extendedTo, x2), core:edge:edge(x3, extendedTo, x4), x = x0, y = x4.
gmarkSubquery02(x, y) <- core:edge:edge(x1, extendedTo, x0), core:edge:edge(x1, publishedIn, x2), core:edge:edge(x2, heldIn, x3), x = x0, y = x3.
gmarkSubquery02(x, y) <- core:edge:edge(x1, extendedTo, x0), core:edge:edge(x1, publishedIn, x2), core:edge:edge(x2, heldIn, x3), x = x0, y = x3.
query(x0, x1, x2) <- gmarkSubquery00(x0, x1), gmarkSubquery01(x1, x2), gmarkSubquery02(x2, x3).
END
