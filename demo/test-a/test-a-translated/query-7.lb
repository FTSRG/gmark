gmarkSubquery03(x, y) <- core:edge:edge(x1, publishedIn, x0), core:edge:edge(x1, publishedIn, x2), core:edge:edge(x3, publishedIn, x2), core:edge:edge(x3, publishedIn, x4), x = x0, y = x4.
gmarkSubquery03(x, y) <- core:edge:edge(x1, publishedIn, x0), core:edge:edge(x1, publishedIn, x2), core:edge:edge(x3, publishedIn, x2), core:edge:edge(x3, publishedIn, x4), x = x0, y = x4.
