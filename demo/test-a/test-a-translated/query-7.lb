BEGIN
gmarkSubquery00(x, y) <- core:edge:edge(x1, extendedTo, x0), core:edge:edge(x2, authors, x1), x = x0, y = x2.
gmarkSubquery00(x, y) <- core:edge:edge(x1, extendedTo, x0), core:edge:edge(x1, publishedIn, x2), core:edge:edge(x3, publishedIn, x2), core:edge:edge(x4, authors, x3), x = x0, y = x4.
gmarkSubquery01(x, y) <- core:edge:edge(x0, authors, x1), core:edge:edge(x1, publishedIn, x2), core:edge:edge(x3, publishedIn, x2), core:edge:edge(x3, publishedIn, x4), x = x0, y = x4.
gmarkSubquery01(x, y) <- core:edge:edge(x0, authors, x1), core:edge:edge(x1, publishedIn, x2), core:edge:edge(x3, publishedIn, x2), core:edge:edge(x3, publishedIn, x4), x = x0, y = x4.
gmarkSubquery01(x, y) <- core:edge:edge(x0, authors, x1), core:edge:edge(x1, publishedIn, x2), x = x0, y = x2.
gmarkSubquery02(x, y) <- core:edge:edge(x1, publishedIn, x0), core:edge:edge(x1, publishedIn, x2), core:edge:edge(x3, publishedIn, x2), core:edge:edge(x3, publishedIn, x4), x = x0, y = x4.
gmarkSubquery02(x, y) <- core:edge:edge(x1, publishedIn, x0), core:edge:edge(x1, publishedIn, x2), x = x0, y = x2.
gmarkSubquery02(x, y) <- core:edge:edge(x1, publishedIn, x0), core:edge:edge(x1, publishedIn, x2), core:edge:edge(x3, publishedIn, x2), core:edge:edge(x3, publishedIn, x4), x = x0, y = x4.
