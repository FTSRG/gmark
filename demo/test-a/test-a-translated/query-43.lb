gmarkSubquery02(x, y) <- core:edge:edge(x0, publishedIn, x1), core:edge:edge(x2, publishedIn, x1), core:edge:edge(x2, publishedIn, x3), core:edge:edge(x4, publishedIn, x3), x = x0, y = x4.
gmarkSubquery03(x, y) <- core:edge:edge(x0, publishedIn, x1), core:edge:edge(x2, publishedIn, x1), core:edge:edge(x2, extendedTo, x3), x = x0, y = x3.
query(x2, x1, x0) <- gmarkSubquery00(x0, x1), gmarkSubquery01(x1, x2), gmarkSubquery02(x2, x3), gmarkSubquery03(x3, x4).
