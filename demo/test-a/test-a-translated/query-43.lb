BEGIN
gmarkSubquery00(x, y) <- core:edge:edge(x1, heldIn, x0), core:edge:edge(x2, publishedIn, x1), x = x0, y = x2.
gmarkSubquery00(x, y) <- core:edge:edge(x1, heldIn, x0), core:edge:edge(x2, publishedIn, x1), core:edge:edge(x2, extendedTo, x3), core:edge:edge(x4, extendedTo, x3), x = x0, y = x4.
