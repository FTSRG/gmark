BEGIN
gmarkSubquery00(x, y) <- core:edge:edge(x0, heldIn, x1), core:edge:edge(x2, heldIn, x1), x = x0, y = x2.
gmarkSubquery01(x, y) <- core:edge:edge(x1, publishedIn, x0), core:edge:edge(x1, publishedIn, x2), core:edge:edge(x3, publishedIn, x2), core:edge:edge(x3, publishedIn, x4), x = x0, y = x4.
gmarkSubquery01(x, x) <- core:edge:edge(x, r, y).
gmarkSubquery01(y, y) <- core:edge:edge(x, r, y).
gmarkSubquery01(x, y) <- gmarkSubquery01(x, z), gmarkSubquery01(z, y).
gmarkSubquery02(x, y) <- core:edge:edge(x1, publishedIn, x0), core:edge:edge(x1, publishedIn, x2), core:edge:edge(x3, publishedIn, x2), core:edge:edge(x3, extendedTo, x4), x = x0, y = x4.
gmarkSubquery02(x, y) <- core:edge:edge(x1, publishedIn, x0), core:edge:edge(x1, extendedTo, x2), x = x0, y = x2.
gmarkSubquery03(x, y) <- core:edge:edge(x1, extendedTo, x0), core:edge:edge(x1, publishedIn, x2), core:edge:edge(x2, heldIn, x3), x = x0, y = x3.
gmarkSubquery03(x, y) <- core:edge:edge(x1, extendedTo, x0), core:edge:edge(x1, publishedIn, x2), core:edge:edge(x2, heldIn, x3), x = x0, y = x3.
query() <- gmarkSubquery00(x0, x1), gmarkSubquery01(x1, x2), gmarkSubquery02(x2, x3), gmarkSubquery03(x3, x4).
END
