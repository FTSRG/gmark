BEGIN
gmarkSubquery00(x, y) <- core:edge:edge(x0, HasKeyword, x1), core:edge:edge(x2, HasKeyword, x1), core:edge:edge(x2, HasKeyword, x3), core:edge:edge(x4, HasKeyword, x3), x = x0, y = x4.
