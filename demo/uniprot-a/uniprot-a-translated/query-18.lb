gmarkSubquery01(x, y) <- core:edge:edge(x1, HasKeyword, x0), core:edge:edge(x1, HasKeyword, x2), x = x0, y = x2.
gmarkSubquery02(x, y) <- core:edge:edge(x1, HasKeyword, x0), core:edge:edge(x1, Interacts, x2), core:edge:edge(x2, HasKeyword, x3), x = x0, y = x3.
query(x1, x3, x2, x0) <- gmarkSubquery00(x0, x1), gmarkSubquery01(x0, x2), gmarkSubquery02(x1, x3).
