<generator>
	<size>2</size>
	<graph>
		<nodes>10</nodes>
	</graph>
	<graph>
		<nodes>20</nodes>
	</graph>




	<predicates>
		<size>3</size>
		<alias symbol="0">authors</alias>
		<proportion symbol="0">0.5</proportion>

		<alias symbol="1">publishedIn</alias>
		<proportion symbol="1">0.5</proportion>

		<alias symbol="2">ietsIn</alias>
		<proportion symbol="2">0.5</proportion>
	</predicates>

	<types>
		<size>4</size>
		<alias type="0">researcher</alias>
		<proportion type="0">1</proportion>

		<alias type="1">paper</alias>
		<proportion type="1">1</proportion>

		<alias type="2">journal</alias>
		<proportion type="2">0.5</proportion>

		<alias type="3">iets</alias>
		<proportion type="3">1</proportion>

	</types>


	<schema>
		<source type="0"> <!-- paper -->
			<target type="1" symbol="0" edge_type="0"> <!-- published in a conference -->
				<indistribution type="gaussian">
					<mu>1</mu>
					<sigma>0</sigma>
				</indistribution>
				<outdistribution type="uniform">
					<min>1</min>
					<max>1</max>
				</outdistribution>
			</target>

			<target type="2" symbol="1" edge_type="1"> <!-- published in a conference -->
				<indistribution type="gaussian">
					<mu>8</mu>
					<sigma>0</sigma>
				</indistribution>
				<outdistribution type="uniform">
					<min>8</min>
					<max>8</max>
				</outdistribution>
			</target>

			<target type="3" symbol="2" edge_type="2"> <!-- extended to a journal -->
				<correlation>
					<edge_type>1</edge_type>
				</correlation>
				<indistribution type="gaussian">
					<mu>5</mu>
					<sigma>1</sigma>
				</indistribution>
				<outdistribution type="uniform">
					<min>4</min>
					<max>6</max>
				</outdistribution>
			</target>
		</source>
	</schema>

	


	<workload id="0" size="50">
		<!-- bounds for the size of these queries -->
		<size>
			<conjuncts min="3" max="4"/>	<!-- number of A^M where M is a multiplicity 1 or * -->
			<disjuncts min="1" max="3"/>	<!-- number of w_1 + ... + w_n in each A -->
			<length min="2" max="4"/>		<!-- number of a_1 . ... . a_m in each w_i -->
		</size>

		<!-- probability to have "*" as multiplicity of the conjuncts -->
		<multiplicity star="0.5"/>

		<!-- the arity of the generated queries, we allow 0 for Boolean queries -->
		<arity min="0" max="4"/>

		<!-- we want only linear queries, but not constant nor quadratic -->
		<selectivity constant="1" linear="1" quadratic="1"/>

		<!-- we want to have in the workload chains and stars, but not cycles, etc. -->
		<type chain="1" star="1" cycle="1" starchain="1"/>
	</workload>

</generator>
