<generator>
	<graph>
		<nodes>30</nodes>
		<output ntriples="1"/>
	</graph>

	<predicates>
		<size>2</size>
		<alias symbol="0">livesIn</alias>
		<proportion symbol="0">0.5</proportion>

		<alias symbol="1">studiesAt</alias>
		<proportion symbol="1">0.5</proportion>
	</predicates>



	<types>
		<size>3</size>
		<alias type="0">person</alias>
		<proportion type="0">0.5</proportion>

		<alias type="1">city</alias>
		<proportion type="1">0.3</proportion>


		<alias type="2">university</alias>
		<proportion type="2">0.2</proportion>
	</types>


	<schema>
		<source type="0"> <!-- person -->
			<target type="0" symbol="0" multiplicity="*" edgeTypeId="0"> <!-- livesIn a city -->
				<outdistribution type="uniform">
					<min>1</min>
					<max>1</max>
				</outdistribution>
				<indistribution type="zipfian">
                    <n>10</n>
					<alpha>2.0</alpha>
				</indistribution>
			</target>

			<target type="2" symbol="1" multiplicity="*" edgeTypeId="1"> <!-- studiesAt a university -->
				<outdistribution type="uniform">
					<min>1</min>
					<max>1</max>
				</outdistribution>
				<indistribution type="zipfian">
                    <n>10</n>
					<alpha>2.0</alpha>
				</indistribution>
			</target>
		</source>
	</schema>



	<!-- we can have several workloads -->
	<!-- for each workload we can generate a specific number of queries -->
	<workload id="0" size="1000">
		<!-- bounds for the size of these queries -->
		<size>
			<conjuncts min="3" max="3"/>	<!-- number of A^M where M is a multiplicity 1 or * -->
			<disjuncts min="1" max="3"/>	<!-- number of w_1 + ... + w_n in each A -->
			<length min="2" max="4"/>		<!-- number of a_1 . ... . a_m in each w_i -->
		</size>

		<!-- probability to have "*" as multiplicity of the conjuncts -->
		<multiplicity star="0.5"/>

		<!-- the arity of the generated queries, we allow 0 for Boolean queries -->
		<arity min="2" max="2"/>

		<!-- we want only empty or linear queries, but not constant nor quadratic -->
		<selectivity empty="1" constant="0" linear="1" quadratic="0"/>

		<!-- we want to have in the workload chains and stars, but not cycles, etc. -->
		<type chain="0" star="1" cycle="0" starchain="0"/>

		<!-- we can choose in which format(s) to generate the workflow -->
		<output sparql="1" postgres="1" cypher="1"/>
	</workload>

</generator>
